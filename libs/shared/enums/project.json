{
  "name": "shared-enums",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/enums/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shared/enums",
        "main": "libs/shared/enums/src/index.ts",
        "tsConfig": "libs/shared/enums/tsconfig.lib.json",
        "assets": [
          "libs/shared/enums/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/shared/enums/**/*.ts",
          "libs/shared/enums/package.json"
        ]
      }
    }
  },
  "tags": [
    "type:shared",
    "type:enums"
  ]
}
