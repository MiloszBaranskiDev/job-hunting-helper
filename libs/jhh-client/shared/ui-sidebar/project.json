    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/jhh-client/shared/ui-sidebar/**/*.ts",
          "libs/jhh-client/shared/ui-sidebar/**/*.html"
        ]
      }
    }
