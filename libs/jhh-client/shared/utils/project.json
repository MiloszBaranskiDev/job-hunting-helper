{
  "name": "jhh-client-shared-utils",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jhh-client/shared/utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/jhh-client/shared/utils",
        "main": "libs/jhh-client/shared/utils/src/index.ts",
        "tsConfig": "libs/jhh-client/shared/utils/tsconfig.lib.json",
        "assets": [
          "libs/jhh-client/shared/utils/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/jhh-client/shared/utils/**/*.ts",
          "libs/jhh-client/shared/utils/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/jhh-client/shared/utils/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
