{
  "name": "jhh-client-shared-pipes",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jhh-client/shared/pipes/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/jhh-client/shared/pipes",
        "main": "libs/jhh-client/shared/pipes/src/index.ts",
        "tsConfig": "libs/jhh-client/shared/pipes/tsconfig.lib.json",
        "assets": [
          "libs/jhh-client/shared/pipes/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/jhh-client/shared/pipes/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [
    "platform:client",
    "type:client-shared"
  ]
}
