{
  "name": "jhh-client-shared-config",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jhh-client/shared/config/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/jhh-client/shared/config",
        "main": "libs/jhh-client/shared/config/src/index.ts",
        "tsConfig": "libs/jhh-client/shared/config/tsconfig.lib.json",
        "assets": [
          "libs/jhh-client/shared/config/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/jhh-client/shared/config/**/*.ts",
          "libs/jhh-client/shared/config/package.json"
        ]
      }
    }
  },
  "tags": [
    "platform:client",
    "type:client-shared"
  ]
}
