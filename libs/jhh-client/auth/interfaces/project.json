{
  "name": "jhh-client-auth-interfaces",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jhh-client/auth/interfaces/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/jhh-client/auth/interfaces",
        "main": "libs/jhh-client/auth/interfaces/src/index.ts",
        "tsConfig": "libs/jhh-client/auth/interfaces/tsconfig.lib.json",
        "assets": ["libs/jhh-client/auth/interfaces/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/jhh-client/auth/interfaces/**/*.ts",
          "libs/jhh-client/auth/interfaces/package.json"
        ]
      }
    }
  },
  "tags": []
}
