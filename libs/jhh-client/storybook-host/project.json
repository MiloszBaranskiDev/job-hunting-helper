{
  "name": "jhh-client-storybook-host",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jhh-client/storybook-host/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/jhh-client/storybook-host",
        "main": "libs/jhh-client/storybook-host/src/index.ts",
        "tsConfig": "libs/jhh-client/storybook-host/tsconfig.lib.json",
        "assets": [
          "libs/jhh-client/storybook-host/*.md"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/jhh-client/storybook-host/.storybook",
        "browserTarget": "jhh-client-storybook-host:build-storybook",
        "compodoc": false,
        "styles": [
          "libs/jhh-client/shared/styles/src/index.scss"
        ],
        "assets": [
          {
            "input": "apps/jhh-client/src/assets",
            "glob": "**/*",
            "output": "assets"
          }
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/jhh-client-storybook-host",
        "configDir": "libs/jhh-client/storybook-host/.storybook",
        "browserTarget": "jhh-client-storybook-host:build-storybook",
        "compodoc": false,
        "styles": [
          "libs/jhh-client/shared/styles/src/index.scss"
        ],
        "assets": [
          {
            "input": "apps/jhh-client/src/assets",
            "glob": "**/*",
            "output": "assets"
          }
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
